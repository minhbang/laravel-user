{
    "name": "minhbang/laravel-user",
    "description": "Laravel User Package",
    "keywords": [
        "minhbang",
        "laravel-user"
    ],
    "homepage": "https://github.com/minhbang/laravel-user",
    "license": "MIT",
    "authors": [
        {
            "name": "Minh Bang",
            "email": "contact@minhbang.com",
            "homepage": "http://minhbang.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/minhbang/laravel-authority.git"
        },
        {
            "type": "vcs",
            "url": "https://gitlab.com/minhbang/laravel-kit.git"
        },
        {
            "type": "vcs",
            "url": "https://gitlab.com/minhbang/laravel-meta.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/minhbang/laravel-setting.git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "minhbang/laravel-kit": "dev-master",
        "minhbang/laravel-authority": "dev-master",
        "minhbang/laravel-meta": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "orchestra/database": "~3.5",
        "nunomaduro/collision": "^1.1",
        "minhbang/laravel-setting": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Minhbang\\User\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Minhbang\\User\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minhbang\\User\\ServiceProvider"
            ]
        }
    }
}
